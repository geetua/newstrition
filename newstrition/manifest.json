{
   "name": "Newstrition",
   "description": "Tracks your media diet",
   "version": "0.0.1",
   "manifest_version": 2,
   "permissions": [ "history", "storage" ],
   "browser_action": {
     "default_icon": "img/icon.jpg",
     "default_popup": "popup.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "web_accessible_resources": [ "mock/*" ]
}
